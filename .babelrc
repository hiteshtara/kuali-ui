{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": ["last 2 versions", "ie >= 11"],
          "node": 6
        }
      }
    ],
    "babel-preset-react"
  ],
  "plugins": [
    "babel-plugin-transform-class-properties",
    "transform-decorators-legacy",
    "babel-plugin-transform-object-rest-spread"
  ]
}
